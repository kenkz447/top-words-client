{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "attach",
            "name": "Attach to Chrome",
            "port": 9222,
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "Debug",
            "type": "chrome",
            "request": "launch",
            "webRoot": "${workspaceRoot}",
            "url": "http://localhost:3333",
            "userDataDir": false,
            "windows": {
                "preLaunchTask": "start",
            }
        }
    ]
}